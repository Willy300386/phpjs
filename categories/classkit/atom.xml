</pre></td><td class='code'><pre><code class='js'><span class='line'><span class="kd">function</span> <span class="nx">classkit_method_copy</span> <span class="p">(</span><span class="nx">dClass</span><span class="p">,</span> <span class="nx">dMethod</span><span class="p">,</span> <span class="nx">sClass</span><span class="p">,</span> <span class="nx">sMethod</span><span class="p">)</span> <span class="p">{</span><span class="o">&lt;</span><span class="err">/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nx">pre</span><span class="o">&gt;&lt;</span><span class="nx">code</span><span class="o">&gt;</span><span class="c1">// http://kevin.vanzonneveld.net</span>
</span><span class='line'><span class="c1">// +   original by: Brett Zamir (http://brett-zamir.me)</span>
</span><span class='line'><span class="c1">// *     example 1: classkit_method_copy(&#39;newClass&#39;, &#39;newMethod&#39;, &#39;someClass&#39;, &#39;someMethod&#39;);</span>
</span><span class='line'><span class="c1">// *     returns 1: true</span>
</span><span class='line'><span class="o">&lt;</span><span class="err">/code&gt;&lt;/pre&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nx">p</span><span class="o">&gt;</span><span class="cm">/*&lt;/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="cm">&lt;pre&gt;&lt;code&gt;function A(){}</span>
</span><span class='line'><span class="cm">function C(){}</span>
</span><span class='line'><span class="cm">C.d = function () {alert(&#39;inside d&#39;);}</span>
</span><span class='line'><span class="cm">classkit_method_copy(&#39;A&#39;, &#39;b&#39;, &#39;C&#39;, &#39;d&#39;);</span>
</span><span class='line'><span class="cm">A.b(); // &#39;inside d&#39;</span>
</span><span class='line'><span class="cm">*/</span>
</span><span class='line'><span class="nx">sMethod</span> <span class="o">=</span> <span class="nx">sMethod</span> <span class="o">||</span> <span class="nx">dMethod</span><span class="p">;</span>
</span><span class='line'>
</span><span class='line'><span class="k">if</span> <span class="p">(</span><span class="k">typeof</span> <span class="nx">dClass</span> <span class="o">===</span> <span class="s1">&#39;string&#39;</span><span class="p">)</span> <span class="p">{</span>
</span><span class='line'>    <span class="nx">dClass</span> <span class="o">=</span> <span class="k">this</span><span class="p">.</span><span class="nb">window</span><span class="p">[</span><span class="nx">dClass</span><span class="p">];</span>
</span><span class='line'><span class="p">}</span>
</span><span class='line'><span class="k">if</span> <span class="p">(</span><span class="k">typeof</span> <span class="nx">sClass</span> <span class="o">===</span> <span class="s1">&#39;string&#39;</span><span class="p">)</span> <span class="p">{</span>
</span><span class='line'>    <span class="nx">sClass</span> <span class="o">=</span> <span class="k">this</span><span class="p">.</span><span class="nb">window</span><span class="p">[</span><span class="nx">sClass</span><span class="p">];</span>
</span><span class='line'><span class="p">}</span>
</span><span class='line'>
</span><span class='line'><span class="c1">//dClass[dMethod] = sClass[sMethod]; // Copy from static to static method (as per PHP example)</span>
</span><span class='line'><span class="nx">dClass</span><span class="p">.</span><span class="nx">prototype</span><span class="p">[</span><span class="nx">dMethod</span><span class="p">]</span> <span class="o">=</span> <span class="nx">sClass</span><span class="p">.</span><span class="nx">prototype</span><span class="p">[</span><span class="nx">sMethod</span><span class="p">];</span> <span class="c1">// To copy from instance to instance</span>
</span><span class='line'><span class="k">return</span> <span class="kc">true</span><span class="p">;</span>
</span><span class='line'><span class="o">&lt;</span><span class="err">/code&gt;&lt;/pre&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nx">p</span><span class="o">&gt;</span><span class="p">}</span>
</span></code></pre></td></tr></table></div></figure></notextile></div></p>

