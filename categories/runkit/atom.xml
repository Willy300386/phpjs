</pre></td><td class='code'><pre><code class='js'><span class='line'><span class="kd">function</span> <span class="nx">runkit_superglobals</span> <span class="p">()</span> <span class="p">{</span><span class="o">&lt;</span><span class="err">/p&gt;</span>
</span><span class='line'>
</span><span class='line'><span class="o">&lt;</span><span class="nx">pre</span><span class="o">&gt;&lt;</span><span class="nx">code</span><span class="o">&gt;</span><span class="c1">// http://kevin.vanzonneveld.net</span>
</span><span class='line'><span class="c1">// +   original by: Brett Zamir (http://brett-zamir.me)</span>
</span><span class='line'><span class="c1">// %          note 1: You must manually define the superglobals to be able to use them.</span>
</span><span class='line'><span class="c1">// %          note 2: Another alternative (though you can&#39;t reflect on them with this function) is to use import_request_variables()</span>
</span><span class='line'><span class="c1">// *     example 1: runkit_superglobals();</span>
</span><span class='line'><span class="c1">// *     returns 1: []</span>
</span><span class='line'><span class="kd">var</span> <span class="nx">superglobal</span> <span class="o">=</span> <span class="p">{},</span>
</span><span class='line'>    <span class="nx">p</span> <span class="o">=</span> <span class="s1">&#39;&#39;</span><span class="p">,</span>
</span><span class='line'>    <span class="nx">arr</span> <span class="o">=</span> <span class="p">[];</span>
</span><span class='line'><span class="kd">var</span> <span class="nx">superglobals</span> <span class="o">=</span> <span class="p">[</span><span class="s1">&#39;$_GET&#39;</span><span class="p">,</span> <span class="s1">&#39;$_POST&#39;</span><span class="p">,</span> <span class="s1">&#39;$_REQUEST&#39;</span><span class="p">,</span> <span class="s1">&#39;$_COOKIE&#39;</span><span class="p">,</span> <span class="s1">&#39;$_SESSION&#39;</span><span class="p">,</span> <span class="s1">&#39;$_SERVER&#39;</span><span class="p">,</span> <span class="s1">&#39;$_ENV&#39;</span><span class="p">,</span> <span class="s1">&#39;$_FILES&#39;</span><span class="p">];</span>
</span><span class='line'><span class="k">for</span> <span class="p">(</span><span class="kd">var</span> <span class="nx">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span> <span class="nx">i</span> <span class="o">&amp;</span><span class="nx">lt</span><span class="p">;</span> <span class="nx">superglobals</span><span class="p">.</span><span class="nx">length</span><span class="p">;</span> <span class="nx">i</span><span class="o">++</span><span class="p">)</span> <span class="p">{</span>
</span><span class='line'>    <span class="nx">superglobal</span> <span class="o">=</span> <span class="k">this</span><span class="p">.</span><span class="nb">window</span><span class="p">[</span><span class="nx">superglobals</span><span class="p">[</span><span class="nx">i</span><span class="p">]];</span>
</span><span class='line'>    <span class="k">for</span> <span class="p">(</span><span class="nx">p</span> <span class="k">in</span> <span class="nx">superglobal</span><span class="p">)</span> <span class="p">{</span>
</span><span class='line'>        <span class="nx">arr</span><span class="p">.</span><span class="nx">push</span><span class="p">(</span><span class="nx">superglobal</span><span class="p">[</span><span class="nx">p</span><span class="p">]);</span>
</span><span class='line'>    <span class="p">}</span>
</span><span class='line'><span class="p">}</span>
</span><span class='line'><span class="k">return</span> <span class="nx">arr</span><span class="p">;</span>
